                            android:layout_height="100dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/ic_c2"
                            android:gravity="center"
                            android:textColor="@color/coloWhite"
                            android:textSize="30sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/txtTotalDevices"
                            android:layout_alignStart="@id/txtTotalDevices"
                            android:layout_alignEnd="@id/txtTotalDevices"
                            android:gravity="center"
                            android:text="@string/total_devices"
                            android:textColor="@color/colorBlack"
                            android:textSize="16sp" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/unassigned"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/client"
                        android:layout_margin="10dp"
                        android:layout_toEndOf="@id/client"
