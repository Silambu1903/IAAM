                            android:layout_height="wrap_content"
                            android:layout_below="@id/txtTotalClients"
                            android:layout_alignStart="@id/txtTotalClients"
                            android:layout_alignEnd="@id/txtTotalClients"
                            android:gravity="center"
                            android:text="@string/total_clients"
                            android:textColor="@color/colorBlack"
                            android:textSize="16sp" />

                    </RelativeLayout>

                    <RelativeLayout
